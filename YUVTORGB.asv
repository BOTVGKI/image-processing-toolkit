function RGB=YUVTORGB(mat)
Y=mat(:,:,1);
U=mat(:,:,2);
V=mat(:,:,3);

%V1=[0.299;0.587;0.114];
%V2=[0.596;-0.274;-0.322];
%V3=[0.211;-0.253;0.312];
%MYIQ=[V1;V2;V3];

%MIYIQ=inv(MYIQ);
%U1=MIYIQ[1];
%U2=MIYIQ[2];
%U3=MIYIQ[3];

R=0.33*Y+0.33*I+0.33*Q;
G=0.5*Y-0.5*Q;
B=0.25*Y+0.5*I-0.25*Q;
RGB(:,:,1)=uint8(R*255);
RGB(:,:,2)=uint8(G*255);
RGB(:,:,3)=uint8(B*255);

end